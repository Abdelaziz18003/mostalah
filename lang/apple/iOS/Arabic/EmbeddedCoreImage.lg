  <File>
    <Filepath>EmbeddedCoreImage/System/Library/Frameworks/CoreImage.framework/en.lproj/Descriptions.strings</Filepath>
    <TextItem>
      <Description/>
      <Position>CIPerspectiveTransform.inputTopRight</Position>
      <TranslationSet>
        <base loc="en">The top right coordinate to map the image to.</base>
        <tran loc="ar">الإحداثي العلوي الأيمن الذي سيتم تعيين الصورة إليه.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIMeshGenerator.inputMesh</Position>
      <TranslationSet>
        <base loc="en">An array of line segments stored as an array of CIVectors each containing a start point and end point.</base>
        <tran loc="ar">مصفوفة من مقاطع خطية مخزنة كمصفوفة من متجهات CIVectors، يحتوي كل منها على نطقة بداية ونقطة نهاية.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIPerspectiveCorrection.inputTopRight</Position>
      <TranslationSet>
        <base loc="en">The top right coordinate to be perspective corrected.</base>
        <tran loc="ar">الإحداثي العلوي الأيمن المراد تصحيح منظوره.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CICrop.inputRectangle</Position>
      <TranslationSet>
        <base loc="en">The rectangle that specifies the crop to apply to the image.</base>
        <tran loc="ar">المستطيل الذي يحدد الاقتصاص الذي سيتم تطبيقه على الصورة.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIPerspectiveTile.inputBottomRight</Position>
      <TranslationSet>
        <base loc="en">The bottom right coordinate of a tile.</base>
        <tran loc="ar">الإحداثي السفلي الأيمن من التجانب.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIBarsSwipeTransition</Position>
      <TranslationSet>
        <base loc="en">Transitions from one image to another by swiping rectangular portions of the foreground image to disclose the target image.</base>
        <tran loc="ar">الانتقال من صورة إلى أخرى عن طريق تحريك أقسام مستطيلة من الصورة الأمامية للكشف عن الصورة المستهدفة.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CISpotColor.inputCloseness3</Position>
      <TranslationSet>
        <base loc="en">A value that indicates how close the third color must match before it is replaced.</base>
        <tran loc="ar">قيمة تشير إلى مدى المطابقة المطلوبة للون الثالث قبل استبداله.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CISwipeTransition.inputOpacity</Position>
      <TranslationSet>
        <base loc="en">The opacity of the swipe.</base>
        <tran loc="ar">معدل تعتيم التحريك.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIHueAdjust</Position>
      <TranslationSet>
        <base loc="en">Changes the overall hue, or tint, of the source pixels.</base>
        <tran loc="ar">تغيير التدرج الكلي للون، أو اللون الخفيف، لوحدات بكسل المصدر.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIMinimumComponent</Position>
      <TranslationSet>
        <base loc="en">Converts an image to grayscale using the minimum of the three color components.</base>
        <tran loc="ar">تحويل صورة إلى التدرج الرمادي باستخدام الحد الأدنى من مكونات الألوان الثلاثة.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIOpTile</Position>
      <TranslationSet>
        <base loc="en">Segments an image, applying any specified scaling and rotation, and then assembles the image again to give an op art appearance.</base>
        <tran loc="ar">تقسيم صورة، وتطبيق أي تغيير في الحجم وتدوير يتم تحديده، ثم تجميع الصورة مرة أخرى لمنحها مظهر الفن البصري.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIStarShineGenerator.inputCrossOpacity</Position>
      <TranslationSet>
        <base loc="en">The opacity of the cross pattern.</base>
        <tran loc="ar">معدل تعتيم النقش المتداخل.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIBokehBlur</Position>
      <TranslationSet>
        <base loc="en">Smooths an image using a disc-shaped convolution kernel.</base>
        <tran loc="ar">تمليس صورة باستخدام مركز التفاف على شكل قرص.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIDiscBlur</Position>
      <TranslationSet>
        <base loc="en">Smooths an image using a disc-shaped convolution kernel.</base>
        <tran loc="ar">تمليس صورة باستخدام مركز التفاف على شكل قرص.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIAffineTile</Position>
      <TranslationSet>
        <base loc="en">Applies an affine transformation to an image and then tiles the transformed image.</base>
        <tran loc="ar">تطبيق تحويل خطي على صورة ثم إجراء تجانب للصورة المحولة.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIParallelogramTile</Position>
      <TranslationSet>
        <base loc="en">Warps an image by reflecting it in a parallelogram, and then tiles the result.</base>
        <tran loc="ar">لإدخال التفاف على صورة عن طريق عكسها في متوازي أضلاع، ثم إجراء تجانب على الناتج.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIAreaMinimumAlpha</Position>
      <TranslationSet>
        <base loc="en">Finds and returns the pixel with the minimum alpha value.</base>
        <tran loc="ar">البحث عن وحدة البكسل ذات الحد الأدنى لقيمة ألفا وإرجاعها.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIMotionBlur</Position>
      <TranslationSet>
        <base loc="en">Blurs an image to simulate the effect of using a camera that moves a specified angle and distance while capturing the image.</base>
        <tran loc="ar">جعل الصورة ضبابية لمحاكاة تأثير استخدام كاميرا تتحرك بزاوية ومسافة محددتين أثناء التقاط الصورة.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIMorphologyRectangleMaximum.inputWidth</Position>
      <TranslationSet>
        <base loc="en">The width in pixels of the morphological operation. The value will be rounded to the nearest odd integer.</base>
        <tran loc="ar">عرض المعالجة الشكلية بالبكسل. سيتم التقريب لأقرب عدد فردي صحيح.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIMorphologyRectangleMinimum.inputWidth</Position>
      <TranslationSet>
        <base loc="en">The width in pixels of the morphological operation. The value will be rounded to the nearest odd integer.</base>
        <tran loc="ar">عرض المعالجة الشكلية بالبكسل. سيتم التقريب لأقرب عدد فردي صحيح.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIDesaturateShadows.inputSoftness</Position>
      <TranslationSet>
        <base loc="en">The softness of the shadow desaturation.</base>
        <tran loc="ar">رقة تخفيف لون الظل.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIConstantColorGenerator.inputColor</Position>
      <TranslationSet>
        <base loc="en">The color to generate.</base>
        <tran loc="ar">اللون الذي سيتم إنشاؤه.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIGlassLozenge.inputPoint1</Position>
      <TranslationSet>
        <base loc="en">The x and y position that defines the center of the circle at the other end of the lozenge.</base>
        <tran loc="ar">الوضع السيني والصادي الذي يحدد مركز الدائرة عند الطرف الآخر من المعين.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>inputAngle</Position>
      <TranslationSet>
        <base loc="en">An angle in radians.</base>
        <tran loc="ar">زاوية بالزاوية نصف القطرية.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIPerspectiveTransform.inputBottomRight</Position>
      <TranslationSet>
        <base loc="en">The bottom right coordinate to map the image to.</base>
        <tran loc="ar">الإحداثي السفلي الأيمن المطلوب تعيين الصورة إليه.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIPaletteCentroid</Position>
      <TranslationSet>
        <base loc="en">Calculate the mean (x,y) image coordinates of a color palette.</base>
        <tran loc="ar">حساب متوسط إحداثيات الصورة (س،ص) للوح الألوان.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CITriangleKaleidoscope.inputPoint</Position>
      <TranslationSet>
        <base loc="en">The x and y position to use as the center of the triangular area in the input image.</base>
        <tran loc="ar">الموضع السيني والصادي الذي سيتم استخدامه كمركز المنطقة مثلثة الشكل في الصورة المُدخلة.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CISpotLight.inputLightPosition</Position>
      <TranslationSet>
        <base loc="en">The x and y position of the spotlight.</base>
        <tran loc="ar">الوضع السيني والصادي للضوء الموضعي.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIColorInvert</Position>
      <TranslationSet>
        <base loc="en">Inverts the colors in an image.</base>
        <tran loc="ar">لعكس الألوان في صورة.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CITriangleKaleidoscope.inputDecay</Position>
      <TranslationSet>
        <base loc="en">The decay determines how fast the color fades from the center triangle.</base>
        <tran loc="ar">يحدد التناقص مدى سرعة تلاشي اللون من مثلث المركز.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIPixellate.inputScale</Position>
      <TranslationSet>
        <base loc="en">The scale determines the size of the squares. Larger values result in larger squares.</base>
        <tran loc="ar">يحدد المقياس حجم المربعات. ينتج عن القيم الأعلى مربعات أكبر.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIMotionBlur.inputAngle</Position>
      <TranslationSet>
        <base loc="en">The angle of the motion determines which direction the blur smears.</base>
        <tran loc="ar">تحدد زاوية الحركة في أي الاتجاهات ستكون آثار الضبابية.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIFourfoldReflectedTile.inputAcuteAngle</Position>
      <TranslationSet>
        <base loc="en">The primary angle for the repeating reflected tile. Small values create thin diamond tiles, and higher values create fatter reflected tiles.</base>
        <tran loc="ar">الزاوية الأساسية للتجانب المعكوس المتكرر. تقوم القيم الصغيرة بإنشاء تجانب رفيع على شكل معين، بينما تقوم القيم الأعلى بإنشاء تجانب معكوس أكثر سمكًا.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIDepthOfField</Position>
      <TranslationSet>
        <base loc="en">Simulates miniaturization effect created by Tilt &amp; Shift lens by performing depth of field effects.</base>
        <tran loc="ar">يحاكي تأثير التصغير الذي يتم إنشاؤه بعدسة الإمالة والإزاحة عن طريق أداء تأثيرات عمق الحقل.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIHighlightShadowAdjust.inputHighlightAmount</Position>
      <TranslationSet>
        <base loc="en">The amount of adjustment to the highlights of the image.</base>
        <tran loc="ar">مقدار الضبط في تمييزات الصورة.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIColorClamp</Position>
      <TranslationSet>
        <base loc="en">Clamp color to a certain range.</base>
        <tran loc="ar">تثبيت اللون على نطاق محدد.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIRadialGradient.inputRadius1</Position>
      <TranslationSet>
        <base loc="en">The radius of the ending circle to use in the gradient.</base>
        <tran loc="ar">نصف قطر دائرة النهاية التي سيتم استخدامها في التدرج.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIPerspectiveTile</Position>
      <TranslationSet>
        <base loc="en">Applies a perspective transform to an image and then tiles the result.</base>
        <tran loc="ar">لتطبيق تحويل منظور على صورة ثم إجراء تجانب للناتج.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>inputCube0Data</Position>
      <TranslationSet>
        <base loc="en">Data containing a 3-dimensional color table of floating-point premultiplied RGBA values. The cells are organized in a standard ordering. The columns and rows of the data are indexed by red and green, respectively. Each data plane is followed by the next higher plane in the data, with planes indexed by blue.</base>
        <tran loc="ar">بيانات تتضمن جدول ألوان ثلاثي الأبعاد ذي قيم RGBA ذات فواصل عائمة مضاعفة مسبقًا. تم تنظيم الخلايا في ترتيب قياسي. تمت فهرسة أعمدة وصفوف البيانات باللونين الأحمر والأخضر على التوالي. يتبع كل مستوى بيانات بالمستوى الأعلى التالي في البيانات، مع فهرسة المستويات باللون الازرق.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>inputCube1Data</Position>
      <TranslationSet>
        <base loc="en">Data containing a 3-dimensional color table of floating-point premultiplied RGBA values. The cells are organized in a standard ordering. The columns and rows of the data are indexed by red and green, respectively. Each data plane is followed by the next higher plane in the data, with planes indexed by blue.</base>
        <tran loc="ar">بيانات تتضمن جدول ألوان ثلاثي الأبعاد ذي قيم RGBA ذات فواصل عائمة مضاعفة مسبقًا. تم تنظيم الخلايا في ترتيب قياسي. تمت فهرسة أعمدة وصفوف البيانات باللونين الأحمر والأخضر على التوالي. يتبع كل مستوى بيانات بالمستوى الأعلى التالي في البيانات، مع فهرسة المستويات باللون الازرق.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>inputCubeData</Position>
      <TranslationSet>
        <base loc="en">Data containing a 3-dimensional color table of floating-point premultiplied RGBA values. The cells are organized in a standard ordering. The columns and rows of the data are indexed by red and green, respectively. Each data plane is followed by the next higher plane in the data, with planes indexed by blue.</base>
        <tran loc="ar">بيانات تتضمن جدول ألوان ثلاثي الأبعاد ذي قيم RGBA ذات فواصل عائمة مضاعفة مسبقًا. تم تنظيم الخلايا في ترتيب قياسي. تمت فهرسة أعمدة وصفوف البيانات باللونين الأحمر والأخضر على التوالي. يتبع كل مستوى بيانات بالمستوى الأعلى التالي في البيانات، مع فهرسة المستويات باللون الازرق.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIModTransition.inputCompression</Position>
      <TranslationSet>
        <base loc="en">The amount of stretching applied to the mod hole pattern. Holes in the center are not distorted as much as those at the edge of the image.</base>
        <tran loc="ar">مقدار التمديد الذي تم تطبيقه على نقش ثقب متوسط. الثقوب الموجودة في المركز غير مشوهة بنفس مقدار تشوه الثقوب الموجودة عند حافة الصورة.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>inputTargetImage</Position>
      <TranslationSet>
        <base loc="en">The target image for a transition.</base>
        <tran loc="ar">الصورة المستهدفة للانتقال.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIBarsSwipeTransition.inputTime</Position>
      <TranslationSet>
        <base loc="en">The parametric time of the transition. This value drives the transition from start (at time 0) to end (at time 1).</base>
        <tran loc="ar">وقت الانتقال المعياري. هذه القيمة تحرك الانتقال من البداية (عند الوقت ٠) إلى النهاية (عند الوقت ١).</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CICopyMachineTransition.inputTime</Position>
      <TranslationSet>
        <base loc="en">The parametric time of the transition. This value drives the transition from start (at time 0) to end (at time 1).</base>
        <tran loc="ar">وقت الانتقال المعياري. هذه القيمة تحرك الانتقال من البداية (عند الوقت ٠) إلى النهاية (عند الوقت ١).</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIDisintegrateWithMaskTransition.inputTime</Position>
      <TranslationSet>
        <base loc="en">The parametric time of the transition. This value drives the transition from start (at time 0) to end (at time 1).</base>
        <tran loc="ar">وقت الانتقال المعياري. هذه القيمة تحرك الانتقال من البداية (عند الوقت ٠) إلى النهاية (عند الوقت ١).</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIDissolveTransition.inputTime</Position>
      <TranslationSet>
        <base loc="en">The parametric time of the transition. This value drives the transition from start (at time 0) to end (at time 1).</base>
        <tran loc="ar">وقت الانتقال المعياري. هذه القيمة تحرك الانتقال من البداية (عند الوقت ٠) إلى النهاية (عند الوقت ١).</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIFlashTransition.inputTime</Position>
      <TranslationSet>
        <base loc="en">The parametric time of the transition. This value drives the transition from start (at time 0) to end (at time 1).</base>
        <tran loc="ar">وقت الانتقال المعياري. هذه القيمة تحرك الانتقال من البداية (عند الوقت ٠) إلى النهاية (عند الوقت ١).</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CIModTransition.inputTime</Position>
      <TranslationSet>
        <base loc="en">The parametric time of the transition. This value drives the transition from start (at time 0) to end (at time 1).</base>
