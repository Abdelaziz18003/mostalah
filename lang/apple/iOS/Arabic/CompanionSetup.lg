      <Position>ABOUT_LOCATION_SERVICES</Position>
      <TranslationSet>
        <base loc="en">What is Location Services</base>
        <tran loc="ar">ما هي خدمات الموقع</tran>
      </TranslationSet>
    </TextItem>
