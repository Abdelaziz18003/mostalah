      <TranslationSet>
        <base loc="en">The attempt to send data to the server “%1$@” failed.</base>
        <tran loc="ar">فشلت محاولة إرسال البيانات إلى الخادم \"%1$@\".</tran>
      </TranslationSet>
    </TextItem>
