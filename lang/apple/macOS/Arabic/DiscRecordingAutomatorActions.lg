  <File>
    <Filepath>DiscRecordingAutomatorActions/System/Library/Automator/Burn A Disc.action/Contents/Resources/en.lproj/Localizable.strings</Filepath>
    <TextItem>
      <Description> Alert buttons </Description>
      <Position>Cancel</Position>
      <TranslationSet>
        <base loc="en">Cancel</base>
        <tran loc="ar">إلغاء</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description> Alert messages </Description>
      <Position>InsertBlank</Position>
      <TranslationSet>
        <base loc="en">Please insert a blank disc with a capacity of at least %@</base>
        <tran loc="ar">الرجاء إدخال قرص فارغ بسعة %@ على الأقل</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>%.1f GB</Position>
      <TranslationSet>
        <base loc="en">%.0f GB</base>
        <tran loc="ar">%.0f غ.ب.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description> Error messages </Description>
      <Position>InsufficientDrive</Position>
      <TranslationSet>
        <base loc="en">The drive you chose can’t burn this much data.</base>
        <tran loc="ar">لا يمكن لمحرك الأقراص المختار نسخ هذا الكم من البيانات.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description> Default name of the disc when not specified </Description>
      <Position>My Disc</Position>
      <TranslationSet>
        <base loc="en">My Disc</base>
        <tran loc="ar">القرص الخاص بي</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description> Capacities </Description>
      <Position>%.0f MB</Position>
      <TranslationSet>
        <base loc="en">%.0f MB</base>
        <tran loc="ar">%.0f م.ب.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>InsertBurned</Position>
      <TranslationSet>
        <base loc="en">Please insert a blank or erasable disc with a capacity of at least %@.</base>
        <tran loc="ar">الرجاء إدخال قرص فارغ أو قابل للمسح بسعة %@ على الأقل.</tran>
      </TranslationSet>
    </TextItem>
  </File>
