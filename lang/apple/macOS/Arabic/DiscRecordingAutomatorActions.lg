  <File>
    <Filepath>DiscRecordingAutomatorActions/System/Library/Automator/Burn A Disc.action/Contents/Resources/en.lproj/InfoPlist.strings</Filepath>
    <TextItem>
      <Description/>
      <Position>AMDSummary</Position>
      <TranslationSet>
        <base loc="en">This action creates a CD or DVD containing the files and folders passed in from the previous action.</base>
        <tran loc="ar">يقوم هذا الإجراء بإنشاء قرص CD أو قرص DVD يحتوي على الملفات والمجلدات التي تم تمريرها من الإجراء السابق.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>AMDOptions</Position>
      <TranslationSet>
        <base loc="en">Name the disc, append today's date to the disc name, choose the device to use.</base>
        <tran loc="ar">تحديد اسم القرص، إلحاق تاريخ اليوم باسم القرص، اختيار الجهاز المراد استخدامه.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description> AMKeywords </Description>
      <Position>CD</Position>
      <TranslationSet>
        <base loc="en">CD</base>
        <tran loc="ar">CD</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>Burn</Position>
      <TranslationSet>
        <base loc="en">Burn</base>
        <tran loc="ar">إنشاء قرص</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description> Localized versions of Info.plist keys </Description>
      <Position>CFBundleName</Position>
      <TranslationSet>
        <base loc="en">BurnADisc</base>
        <tran loc="ar">BurnADisc</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>AMName</Position>
      <TranslationSet>
        <base loc="en">Burn a Disc</base>
        <tran loc="ar">إنشاء قرص</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>Folder</Position>
      <TranslationSet>
        <base loc="en">Folder</base>
        <tran loc="ar">مجلد</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>AMDInput</Position>
      <TranslationSet>
        <base loc="en">Files and folders passed from the previous action.</base>
        <tran loc="ar">الملفات والمجلدات التي تم تمريرها من الإجراء السابق.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>AMDResult</Position>
      <TranslationSet>
        <base loc="en">Files and folders passed from the previous action.</base>
        <tran loc="ar">الملفات والمجلدات التي تم تمريرها من الإجراء السابق.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>File</Position>
      <TranslationSet>
        <base loc="en">File</base>
        <tran loc="ar">ملف</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>DVD</Position>
      <TranslationSet>
        <base loc="en">DVD</base>
        <tran loc="ar">قرص DVD</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>AMDRequires</Position>
      <TranslationSet>
        <base loc="en">A drive capable of burning and a blank disc.</base>
        <tran loc="ar">محرك أقراص قادر على الإنشاء وقرص فارغ.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>title</Position>
      <TranslationSet>
        <base loc="en">My Disc</base>
        <tran loc="ar">القرص الخاص بي</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description> AMCategory </Description>
      <Position>Utility</Position>
      <TranslationSet>
        <base loc="en">Utility</base>
        <tran loc="ar">أداة مساعدة</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>NSHumanReadableCopyright</Position>
      <TranslationSet>
        <base loc="en">Copyright (c) 2004-2012 Apple Inc.  All Rights Reserved.</base>
        <tran loc="ar">‏‎Copyright (c) 2004-2012 Apple Inc.‎\n‏ كافة الحقوق محفوظة.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description> AMApplication </Description>
      <Position>System</Position>
      <TranslationSet>
        <base loc="en">System</base>
        <tran loc="ar">النظام</tran>
      </TranslationSet>
    </TextItem>
